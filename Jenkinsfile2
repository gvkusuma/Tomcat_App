pipeline
{
    agent any
    stages{ 
        stage('Init')
        {
	steps{
            echo "Starting the pipeline testing"
	    }
        }
        stage('Build')
        {
	steps{
            echo "Build the project"
        }
	}
        stage('Deploy')
        {
	steps{
            echo "Deploy the project"
        }
	}
    }
}
